<%- include partals/headder.ejs %> 

<%- include partals/main_nav.ejs %> 

<main class="container">

    <div class="row">
        <div class="col-md-offset-3 col-md-6"></div>
        <h1><%= title  %> </h1>
        <br>
       <table class ="table table-bordered table-striped table-hover">
            <!--header-->
            <tr>
                <th class = "text-center">Name</th>
                <th class = "text-center">contact number</th>
                <th class = "text-center">email</th>
            </tr>
        <!--repeatable-->
            <% for(let count = 0; count < PageLists.length; count++) { %>

                <tr>
                    <th class = "text-center"><%= PageLists[count].contact_name %></th>
                    <th class = "text-center"><%= PageLists[count].contact_number %></th>
                    <th class = "text-center"><%= PageLists[count].email %></th>
                </tr>


                <% }%>


        </table>

    </div>
</main>



